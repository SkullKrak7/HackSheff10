{
  "dashboard": {
    "title": "RetailOdyssey Agent Metrics",
    "panels": [
      {
        "id": 1,
        "title": "Total Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "retail_odyssey_total_requests"
          }
        ]
      },
      {
        "id": 2,
        "title": "Agent Call Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "retail_odyssey_agent_calls"
          }
        ]
      },
      {
        "id": 3,
        "title": "Average Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "retail_odyssey_avg_response_time"
          }
        ]
      }
    ]
  }
}
